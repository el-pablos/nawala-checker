# Nawala Checker Configuration
# Copy these settings to your .env file

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Cache Configuration (optional, defaults to file)
CACHE_DRIVER=redis
CACHE_PREFIX=nawala_

# Queue Configuration (recommended for background processing)
QUEUE_CONNECTION=redis

# Redis Configuration (if using redis for cache/queue)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug

# Application
APP_NAME="Nawala Checker"
APP_ENV=production
APP_DEBUG=false
APP_URL=http://localhost

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=nawala_checker
DB_USERNAME=root
DB_PASSWORD=

